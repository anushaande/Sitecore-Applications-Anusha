@using Sitecore.Mvc
@{
    if (Sitecore.Context.PageMode.IsPageEditor)
    {
        <script>
            jQuery(document).ready(function () {
                autoSave();
            });
        </script>
        <style>.col-left-adjust { margin-top: 40px; }</style>
    }
}
<div class="row">
    <div class="col-sm-3 col-left-adjust">
        <!-- left nav root element goes here -->
        @Html.Sitecore().Placeholder("Vertical Navigation")
        @Html.Sitecore().Placeholder("Contact Information")
    </div>
    <div class="col-sm-9 col-right-adjust">
        @Html.Sitecore().Placeholder("Faculty Affairs Page Type")
    </div>
</div>